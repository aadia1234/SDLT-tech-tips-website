
{
    "name": "Tech Tips",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts" : [
        {
            "matches": ["*://*/*"],
            "js" : ["content.js"],
            "all_frames": true
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Tech Tips"
    }
}
